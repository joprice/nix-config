{
  "coc.preferences.formatOnSaveFiletypes": [
    "scala",
    "haskell",
    "json",
    "nix",
    "ocaml",
    "javascript",
    "typescript",
    "typescriptreact",
    "dart",
    "ruby"
  ],
  "languageserver": {
    "nix": {
      "command": "rnix-lsp",
      "filetypes": [
        "nix"
      ]
    },
    "ocaml-lsp": {
        "command": "ocamllsp",
        "filetypes": [
          "ocaml",
          "reason"
        ]
    }
  }
}
